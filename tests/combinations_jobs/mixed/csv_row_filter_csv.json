{
  "name": "CSV Row Filter to CSV",
  "num_of_retries": 0,
  "file_logging": false,
  "strategy_type": "row",
  "metadata": { "created_by": "pytest" },
  "components": [
    {
      "name": "reader",
      "comp_type": "read_csv",
      "description": "CSV in (row)",
      "filepath": "__IN__",
      "next": ["flt"]
    },
    {
      "name": "flt",
      "comp_type": "filter",
      "description": "keep names != Bob",
      "rule": { "column": "name", "operator": "!=", "value": "Bob" },
      "next": ["writer"]
    },
    {
      "name": "writer",
      "comp_type": "write_csv",
      "description": "CSV out",
      "filepath": "__OUT__"
    }
  ]
}
