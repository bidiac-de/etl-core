{
  "name": "CSV Bulk Filter to Excel",
  "num_of_retries": 0,
  "file_logging": false,
  "strategy_type": "bulk",
  "metadata": { "created_by": "pytest" },
  "components": [
    {
      "name": "reader",
      "comp_type": "read_csv",
      "description": "CSV in (bulk)",
      "filepath": "__IN__",
      "next": ["flt"]
    },
    {
      "name": "flt",
      "comp_type": "filter",
      "description": "name != Bob",
      "rule": { "column": "name", "operator": "!=", "value": "Bob" },
      "next": ["writer"]
    },
    {
      "name": "writer",
      "comp_type": "write_excel",
      "description": "Excel out",
      "filepath": "__OUT__"
    }
  ]
}
