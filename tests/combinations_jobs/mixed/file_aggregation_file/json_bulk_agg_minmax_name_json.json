{
  "name": "JSON Bulk Aggregation (min/max name) to JSON",
  "num_of_retries": 0,
  "file_logging": false,
  "strategy_type": "bulk",
  "metadata": { "created_by": "pytest" },
  "components": [
    {
      "name": "reader",
      "comp_type": "read_json",
      "description": "JSON array in (bulk)",
      "filepath": "__IN__",
      "routes": { "out": [ { "to": "agg", "in_port": "in" } ] },
      "out_port_schemas": {
        "out": { "fields": [
          { "name": "id", "data_type": "string" },
          { "name": "name", "data_type": "string" }
        ] }
      }
    },
    {
      "name": "agg",
      "comp_type": "aggregation",
      "description": "min/max(name) without grouping",
      "group_by": [],
      "aggregations": [
        { "src": "name", "op": "min", "dest": "name_min" },
        { "src": "name", "op": "max", "dest": "name_max" }
      ],
      "in_port_schemas": {
        "in": { "fields": [
          { "name": "id", "data_type": "string" },
          { "name": "name", "data_type": "string" }
        ] }
      },
      "routes": { "out": [ { "to": "writer", "in_port": "in" } ] },
      "out_port_schemas": {
        "out": { "fields": [
          { "name": "name_min", "data_type": "string" },
          { "name": "name_max", "data_type": "string" }
        ] }
      }
    },
    {
      "name": "writer",
      "comp_type": "write_json",
      "description": "JSON array out",
      "filepath": "__OUT__",
      "in_port_schemas": {
        "in": { "fields": [
          { "name": "name_min", "data_type": "string" },
          { "name": "name_max", "data_type": "string" }
        ] }
      }
    }
  ]
}
